<?xml version="1.0" encoding="utf-8"?>
<form>
  <fieldset>
    <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="ordering"/>
    <field name="state" default="1" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1">
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" default="0000-00-00 00:00:00" type="hidden" filter="unset"/>
    <field name="created_by" type="createdby" default="0" label="COM_OBJECTIVES_FORM_LBL_OBJECTIVES_CREATED_BY" description="COM_OBJECTIVES_FORM_DESC_OBJECTIVES_CREATED_BY" hidden="true" hint="COM_OBJECTIVES_FORM_LBL_OBJECTIVES_CREATED_BY"/>
    <field name="modified_by" type="modifiedby" default="0" label="COM_OBJECTIVES_FORM_LBL_OBJECTIVES_MODIFIED_BY" description="COM_OBJECTIVES_FORM_DESC_OBJECTIVES_MODIFIED_BY" hidden="true" hint="COM_OBJECTIVES_FORM_LBL_OBJECTIVES_MODIFIED_BY"/>
    <field name="response_type" filter="safehtml" type="text" label="COM_OBJECTIVES_FORM_LBL_OBJECTIVES_RESPONSE_TYPE" description="COM_OBJECTIVES_FORM_DESC_OBJECTIVES_RESPONSE_TYPE" hint="COM_OBJECTIVES_FORM_LBL_OBJECTIVES_RESPONSE_TYPE"/>
    <field name="category" extension="com_objectives" type="category" required="true" message="Please Select Category" label="COM_OBJECTIVES_FORM_LBL_OBJECTIVES_CATEGORY" description="COM_OBJECTIVES_FORM_DESC_OBJECTIVES_CATEGORY" hint="COM_OBJECTIVES_FORM_LBL_OBJECTIVES_CATEGORY"/>
    <field name="target_score" filter="safehtml" type="text" required="true" message="Please enter target score" label="COM_OBJECTIVES_FORM_LBL_OBJECTIVES_TARGET_SCORE" description="COM_OBJECTIVES_FORM_DESC_OBJECTIVES_TARGET_SCORE" hint="COM_OBJECTIVES_FORM_LBL_OBJECTIVES_TARGET_SCORE"/>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
  </fieldset>
</form>
