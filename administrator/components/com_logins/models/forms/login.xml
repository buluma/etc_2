<?xml version="1.0" encoding="utf-8"?>
<form>
  <fieldset>
    <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="ordering"/>
    <field name="state" default="1" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1">
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" default="0000-00-00 00:00:00" type="hidden" filter="unset"/>
    <field name="created_by" type="createdby" default="0" label="COM_LOGINS_FORM_LBL_LOGIN_CREATED_BY" description="COM_LOGINS_FORM_DESC_LOGIN_CREATED_BY" hidden="true" hint="COM_LOGINS_FORM_LBL_LOGIN_CREATED_BY"/>
    <field name="modified_by" type="modifiedby" default="0" label="COM_LOGINS_FORM_LBL_LOGIN_MODIFIED_BY" description="COM_LOGINS_FORM_DESC_LOGIN_MODIFIED_BY" hidden="true" hint="COM_LOGINS_FORM_LBL_LOGIN_MODIFIED_BY"/>
    <field name="username" type="user" label="COM_LOGINS_FORM_LBL_LOGIN_USERNAME" description="COM_LOGINS_FORM_DESC_LOGIN_USERNAME" hint="COM_LOGINS_FORM_LBL_LOGIN_USERNAME"/>
    <field name="login_time" default="0000-00-00 00:00:00" label="COM_LOGINS_FORM_LBL_LOGIN_LOGIN_TIME" description="COM_LOGINS_FORM_DESC_LOGIN_LOGIN_TIME" hint="COM_LOGINS_FORM_LBL_LOGIN_LOGIN_TIME"/>
    <field name="day" type="timecreated" label="COM_LOGINS_FORM_LBL_LOGIN_DAY" description="COM_LOGINS_FORM_DESC_LOGIN_DAY" hint="COM_LOGINS_FORM_LBL_LOGIN_DAY"/>
    <field name="login_coords" filter="safehtml" type="text" label="COM_LOGINS_FORM_LBL_LOGIN_LOGIN_COORDS" description="COM_LOGINS_FORM_DESC_LOGIN_LOGIN_COORDS" hint="COM_LOGINS_FORM_LBL_LOGIN_LOGIN_COORDS"/>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
  </fieldset>
</form>
