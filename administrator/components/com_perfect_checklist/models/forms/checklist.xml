<?xml version="1.0" encoding="utf-8"?>
<form>
  <fieldset>
    <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="ordering"/>
    <field name="state" default="1" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1">
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" default="0000-00-00 00:00:00" type="hidden" filter="unset"/>
    <field name="created_by" type="createdby" label="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_CREATED_BY" description="COM_PERFECT_CHECKLIST_FORM_DESC_CHECKLIST_CREATED_BY" hidden="true" hint="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_CREATED_BY"/>
    <field name="modified_by" type="modifiedby" default="0" label="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_MODIFIED_BY" description="COM_PERFECT_CHECKLIST_FORM_DESC_CHECKLIST_MODIFIED_BY" hidden="true" hint="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_MODIFIED_BY"/>
    <field name="inputdate" filter="safehtml" type="text" label="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_INPUTDATE" description="COM_PERFECT_CHECKLIST_FORM_DESC_CHECKLIST_INPUTDATE" hidden="true" hint="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_INPUTDATE"/>
    <field name="client_id" filter="safehtml" type="text" label="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_CLIENT_ID" description="COM_PERFECT_CHECKLIST_FORM_DESC_CHECKLIST_CLIENT_ID" hidden="true" hint="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_CLIENT_ID"/>
    <field name="shop_mml" filter="safehtml" type="text" label="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_SHOP_MML" description="COM_PERFECT_CHECKLIST_FORM_DESC_CHECKLIST_SHOP_MML" hint="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_SHOP_MML"/>
    <field name="sku_available" default="1" type="list" label="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_SKU_AVAILABLE" description="COM_PERFECT_CHECKLIST_FORM_DESC_CHECKLIST_SKU_AVAILABLE" hint="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_SKU_AVAILABLE">
      <option value="1">COM_PERFECT_CHECKLIST_CHECKLISTS_SKU_AVAILABLE_OPTION_1</option>
      <option value="2">COM_PERFECT_CHECKLIST_CHECKLISTS_SKU_AVAILABLE_OPTION_2</option>
    </field>
    <field name="merchandising" filter="safehtml" type="text" label="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_MERCHANDISING" description="COM_PERFECT_CHECKLIST_FORM_DESC_CHECKLIST_MERCHANDISING" hint="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_MERCHANDISING"/>
    <field name="shelf_quantity" filter="safehtml" type="text" label="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_SHELF_QUANTITY" description="COM_PERFECT_CHECKLIST_FORM_DESC_CHECKLIST_SHELF_QUANTITY" hint="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_SHELF_QUANTITY"/>
    <field name="right_prices" filter="safehtml" type="text" label="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_RIGHT_PRICES" description="COM_PERFECT_CHECKLIST_FORM_DESC_CHECKLIST_RIGHT_PRICES" hint="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_RIGHT_PRICES"/>


    <field name="traffic_area" filter="safehtml" type="text" label="Placed in high traffic area" description="Placed in high traffic area" hint="Placed in high traffic area"/>

    <field name="level_eye" filter="safehtml" type="text" label="Placed in eye level" description="Placed in eye level" hint="Placed in eye level"/>

    <field name="priority_gondolas" filter="safehtml" type="text" label="Gondolas" description="Gondolas" hint="Gondolas"/>

    <field name="posm" filter="safehtml" type="text" label="Posm" description="Posm" hint="Posm"/>

    <field name="retailer_informed" filter="safehtml" type="text" label="Retailer Informed" description="Retailer Informed" hint="Retailer Informed"/>

    <field name="promotion_visible" filter="safehtml" type="text" label="promotion_visible" description="promotion_visible" hint="promotion_visible"/>

    <field name="wobbler" filter="safehtml" type="text" label="wobbler" description="wobbler" hint="wobbler"/>

    <field name="posters_place" filter="safehtml" type="text" label="posters_place" description="posters_place" hint="posters_place"/>

    <field name="gondolas_installed" filter="safehtml" type="text" label="gondolas_installed" description="gondolas_installed" hint="gondolas_installed"/>

    <field name="user_id" type="user" label="user_id" description="user_id" hint="user_id"/>

    <field name="client_modified_date" type="calendar" label="client_modified_date" description="client_modified_date" hint="client_modified_date" readonly="true"/>

    <field name="created" filter="safehtml" type="text" label="created" description="created" hint="created" readonly="true"/>

    <field name="last_sync_date" filter="safehtml" type="last_sync_date" label="last_sync_date" description="last_sync_date" hint="last_sync_date" readonly="true"/>

    <field name="posm" filter="safehtml" type="text" label="Posm" description="Posm" hint="Posm"/>
    

    <field name="visible_tags" filter="safehtml" type="text" label="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_VISIBLE_TAGS" description="COM_PERFECT_CHECKLIST_FORM_DESC_CHECKLIST_VISIBLE_TAGS" hint="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_VISIBLE_TAGS"/>
    <field name="store_id" query="SELECT id, shop_name FROM `#__outlets`" key_field="id" value_field="shop_name" type="sql" label="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_STORE_ID" description="COM_PERFECT_CHECKLIST_FORM_DESC_CHECKLIST_STORE_ID" readonly="true" hint="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_STORE_ID"/>
    <field name="dateformat" class="inputbox" timeformat="12" filter="server_utc" showtime="false" singleheader="false" todaybutton="true" weeknumbers="true" filltable="true" type="calendar" label="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_DATEFORMAT" readonly="true" description="COM_PERFECT_CHECKLIST_FORM_DESC_CHECKLIST_DATEFORMAT" hint="COM_PERFECT_CHECKLIST_FORM_LBL_CHECKLIST_DATEFORMAT"/>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
  </fieldset>
  <fieldset name="accesscontrol">
    <field name="asset_id" type="hidden" filter="unset"/>
    <field name="rules" type="rules" label="JFIELD_RULES_LABEL" translate_label="false" filter="rules" validate="rules" class="inputbox" component="com_perfect_checklist" section="checklist"/>
  </fieldset>
</form>
