<?xml version="1.0" encoding="utf-8"?>
<form>
  <fieldset>
    <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="ordering"/>
    <field name="state" default="1" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1">
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" default="0000-00-00 00:00:00" type="hidden" filter="unset"/>
    <field name="created_by" type="createdby" default="0" label="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_CREATED_BY" description="COM_OBJECTIVES_FORM_DESC_OBJECTIVE_CREATED_BY" hidden="true" hint="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_CREATED_BY"/>
    <field name="modified_by" type="modifiedby" default="0" label="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_MODIFIED_BY" description="COM_OBJECTIVES_FORM_DESC_OBJECTIVE_MODIFIED_BY" hidden="true" hint="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_MODIFIED_BY"/>
    
    <field name="client_id" filter="safehtml" type="text" label="client_id" description="client_id" hint="client_id" readonly="true"/>

    <field name="inputdate" filter="safehtml" type="text" label="Date Submitted" description="Date Submitted" hint="Date Submitted" readonly="true"/>

    <field name="targetscore" filter="safehtml" type="text" label="Target Score" description="Target Score" hint="Target Score" />

    <field name="categorytotal" filter="safehtml" type="text" label="Category Total" description="Category Total" hint="Category Total"/>

    <field name="targetfacings" filter="safehtml" type="text" label="Target Facings" description="Target Facings" hint="Target Facings"/>

    <field name="current_percent" filter="safehtml" type="text" label="Current Percent" description="Current Percent" hint="Current Percent"/>

    <field name="current_facings" filter="safehtml" type="text" label="current Facings" description="current Facings" hint="Target Facings"/>

    <field name="objective_achieved" filter="safehtml" type="text" label="objective achieved" description="objective achieved" hint="objective achieved"/>

    <field name="reason_not_achieved" filter="safehtml" type="textarea" label="Reason for failure" description="reason for objective not being achieved" hint="Reason for failure"/>

    <field name="action_point" filter="safehtml" type="textarea" label="Action Point" description="Action Point" hint="Action Point"/>

    <field name="submitter" type="createdby" label="COM_VOC_FORM_LBL_VOC_SUBMITTER" description="COM_VOC_FORM_DESC_VOC_SUBMITTER" hidden="true" hint="COM_VOC_FORM_LBL_VOC_SUBMITTER"/>
    <field name="user_id" type="createdby" label="COM_VOC_FORM_LBL_VOC_USER_ID" description="COM_VOC_FORM_DESC_VOC_USER_ID" hidden="true" hint="COM_VOC_FORM_LBL_VOC_USER_ID"/>
    <field name="last_sync_date" filter="safehtml" type="text" readonly="true" label="Last Sync Date" description="Last Sync Date" hint="Last Sync Date"/>

    <field name="server_modified_date" filter="safehtml" type="text" readonly="true" label="Server Modified Date" description="Server Modified Date" hint="Server Modified Date record was modified"/>

    <field name="coordinates" filter="safehtml" type="text" readonly="true" label="coordinates" description="coordinates" hint="coordinates"/>

    <field name="first_insert" filter="safehtml" type="text" readonly="true" label="First Insert Date" description="First Insert" hint="First Insert"/>

    <field name="created" filter="safehtml" type="timecreated" readonly="true" label="Date Created" description="Date Created" hint="Date Created"/>

    <field name="client_modified_date" filter="safehtml" type="text" readonly="true" label="Client Modified Date" description="Client Modified Date" hint="Client Modified Date"/>

    <field name="objective_desc" filter="safehtml" type="text" label="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_OBJECTIVE" description="COM_OBJECTIVES_FORM_DESC_OBJECTIVE_OBJECTIVE" hint="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_OBJECTIVE"/>
    <field name="response_type" type="list" label="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_RESPONSE_TYPE" description="COM_OBJECTIVES_FORM_DESC_OBJECTIVE_RESPONSE_TYPE" hint="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_RESPONSE_TYPE">
      <option value="score">COM_OBJECTIVES_OBJECTIVES_RESPONSE_TYPE_OPTION_SCORE</option>
      <option value="agree">COM_OBJECTIVES_OBJECTIVES_RESPONSE_TYPE_OPTION_AGREE</option>
    </field>
    <field name="category" extension="com_objectives" type="category" required="true" message="Please Select Category" label="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_CATEGORY" description="COM_OBJECTIVES_FORM_DESC_OBJECTIVE_CATEGORY" hint="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_CATEGORY"/>
    <field name="target_score" filter="safehtml" type="text" label="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_TARGET_SCORE" description="COM_OBJECTIVES_FORM_DESC_OBJECTIVE_TARGET_SCORE" hint="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_TARGET_SCORE" showon="response_type:score"/>
    <field name="published" default="1" type="radio" label="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_PUBLISHED" description="COM_OBJECTIVES_FORM_DESC_OBJECTIVE_PUBLISHED" hint="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_PUBLISHED">
      <option value="1">COM_OBJECTIVES_OBJECTIVES_PUBLISHED_OPTION_1</option>
      <option value="0">COM_OBJECTIVES_OBJECTIVES_PUBLISHED_OPTION_0</option>
    </field>
    <field name="deleted" default="0" type="radio" label="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_DELETED" description="COM_OBJECTIVES_FORM_DESC_OBJECTIVE_DELETED" hint="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_DELETED">
      <option value="1">COM_OBJECTIVES_OBJECTIVES_DELETED_OPTION_1</option>
      <option value="0">COM_OBJECTIVES_OBJECTIVES_DELETED_OPTION_0</option>
    </field>
    <field name="created_on" default="0000-00-00 00:00:00" readonly="true" label="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_CREATED_ON" description="COM_OBJECTIVES_FORM_DESC_OBJECTIVE_CREATED_ON" hint="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_CREATED_ON"/>
    <field name="modified_on" default="0000-00-00 00:00:00" readonly="true" label="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_MODIFIED_ON" description="COM_OBJECTIVES_FORM_DESC_OBJECTIVE_MODIFIED_ON" hint="COM_OBJECTIVES_FORM_LBL_OBJECTIVE_MODIFIED_ON"/>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
  </fieldset>
</form>
