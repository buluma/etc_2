<?xml version="1.0" encoding="utf-8"?>
<form>
  <fieldset>
    <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="client_id" filter="safehtml" type="text" readonly="true" label="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_CLIENT_ID" description="COM_OTHER_OBJECTIVES_FORM_DESC_OTHER_OBJECTIVE_CLIENT_ID" hint="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_CLIENT_ID"/>
    <field name="ordering"/>
    <field name="state" default="1" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1">
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" default="0000-00-00 00:00:00" type="hidden" filter="unset"/>
    <field name="objective" filter="safehtml" type="text" label="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_OBJECTIVE" description="COM_OTHER_OBJECTIVES_FORM_DESC_OTHER_OBJECTIVE_OBJECTIVE" hint="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_OBJECTIVE"/>
    <field name="objective_achieved" default="1" type="radio" label="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_OBJECTIVE_ACHIEVED" description="COM_OTHER_OBJECTIVES_FORM_DESC_OTHER_OBJECTIVE_OBJECTIVE_ACHIEVED" hint="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_OBJECTIVE_ACHIEVED">
      <option value="1">COM_OTHER_OBJECTIVES_OTHER_OBJECTIVES_OBJECTIVE_ACHIEVED_OPTION_1</option>
      <option value="0">COM_OTHER_OBJECTIVES_OTHER_OBJECTIVES_OBJECTIVE_ACHIEVED_OPTION_0</option>
    </field>
    <field name="inputdate" type="timecreated" label="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_INPUTDATE" description="COM_OTHER_OBJECTIVES_FORM_DESC_OTHER_OBJECTIVE_INPUTDATE" hint="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_INPUTDATE"/>
    <field name="challenge" filter="safehtml" type="textarea" label="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_CHALLENGE" description="COM_OTHER_OBJECTIVES_FORM_DESC_OTHER_OBJECTIVE_CHALLENGE" hint="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_CHALLENGE"/>
    <field name="next_plan" filter="safehtml" type="textarea" label="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_NEXT_PLAN" description="COM_OTHER_OBJECTIVES_FORM_DESC_OTHER_OBJECTIVE_NEXT_PLAN" hint="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_NEXT_PLAN"/>
    <field name="store" query="SELECT id as value, shop_name as text from #__outlets" key_field="value" value_field="text" type="sql" label="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_STORE" description="COM_OTHER_OBJECTIVES_FORM_DESC_OTHER_OBJECTIVE_STORE" hint="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_STORE"/>
    <field name="store_id" query="SELECT id as value, shop_name as text from #__outlets" key_field="value" value_field="text" type="sql" label="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_STORE_ID" description="COM_OTHER_OBJECTIVES_FORM_DESC_OTHER_OBJECTIVE_STORE_ID" hint="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_STORE_ID"/>
    <field name="store_server_id" query="SELECT id as value, shop_name as text from #__outlets" key_field="value" value_field="text" type="sql" label="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_STORE_SERVER_ID" description="COM_OTHER_OBJECTIVES_FORM_DESC_OTHER_OBJECTIVE_STORE_SERVER_ID" hint="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_STORE_SERVER_ID"/>
    <field name="created_on" default="0000-00-00 00:00:00" label="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_CREATED_ON" description="COM_OTHER_OBJECTIVES_FORM_DESC_OTHER_OBJECTIVE_CREATED_ON" hint="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_CREATED_ON"/>
    <field name="coordinates" filter="safehtml" type="text" readonly="true" label="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_COORDINATES" description="COM_OTHER_OBJECTIVES_FORM_DESC_OTHER_OBJECTIVE_COORDINATES" hint="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_COORDINATES"/>
    <field name="created" type="timecreated" label="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_CREATED" description="COM_OTHER_OBJECTIVES_FORM_DESC_OTHER_OBJECTIVE_CREATED" hint="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_CREATED"/>
    <field name="first_insert" default="0000-00-00 00:00:00" label="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_FIRST_INSERT" description="COM_OTHER_OBJECTIVES_FORM_DESC_OTHER_OBJECTIVE_FIRST_INSERT" hint="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_FIRST_INSERT"/>
    <field name="client_modified_date" type="timecreated" label="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_CLIENT_MODIFIED_DATE" description="COM_OTHER_OBJECTIVES_FORM_DESC_OTHER_OBJECTIVE_CLIENT_MODIFIED_DATE" hint="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_CLIENT_MODIFIED_DATE"/>
    <field name="modified_on" default="0000-00-00 00:00:00" label="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_MODIFIED_ON" description="COM_OTHER_OBJECTIVES_FORM_DESC_OTHER_OBJECTIVE_MODIFIED_ON" hint="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_MODIFIED_ON"/>
    <field name="server_modified_date" type="timecreated" label="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_SERVER_MODIFIED_DATE" description="COM_OTHER_OBJECTIVES_FORM_DESC_OTHER_OBJECTIVE_SERVER_MODIFIED_DATE" hint="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_SERVER_MODIFIED_DATE"/>
    <field name="last_sync_date" filter="safehtml" type="text" readonly="true" label="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_LAST_SYNC_DATE" description="COM_OTHER_OBJECTIVES_FORM_DESC_OTHER_OBJECTIVE_LAST_SYNC_DATE" hint="COM_OTHER_OBJECTIVES_FORM_LBL_OTHER_OBJECTIVE_LAST_SYNC_DATE"/>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
  </fieldset>
</form>
